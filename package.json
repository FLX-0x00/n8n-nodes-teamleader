{
  "name": "n8n-nodes-teamleader",
  "version": "1.2.8",
  "description": "n8n community node for Teamleader CRM",
  "keywords": [
    "n8n-community-node-package",
    "n8n-nodes",
    "n8n",
    "teamleader",
    "crm",
    "n8n-nodes",
    "n8n",
    "n8n.io",
    "n8n.io-nodes"
  ],
  "license": "MIT",
  "homepage": "https://www.npmjs.com/package/n8n-nodes-teamleader",
  "author": {
    "name": "Paul Werther",
    "email": "flx@evait.de"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FLX-0x00/n8n-nodes-teamleader.git"
  },
  "packageManager": "pnpm@10.22.0",
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TeamleaderOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Teamleader/TeamleaderTrigger.node.js",
      "dist/nodes/Teamleader/Teamleader.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
